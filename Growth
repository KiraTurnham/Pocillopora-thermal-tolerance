```{r}
outlier<-bw %>%
group_by(sym.spp,time_point) %>%
identify_outliers(weight)
data.frame(outlier)

Q1 <- quantile(bw$weight, .25)
Q3 <- quantile(bw$weight, .75)
IQR <- IQR(bw$weight)

no_outliers <- subset(bw, bw$weight> (Q1 - 1.5*IQR) & bw$weight< (Q3 + 1.5*IQR))

outliers<-no_outliers %>%
group_by(sym.spp,time_point) %>%
identify_outliers(weight)
data.frame(outliers)

no_outlier<- bw[-c()]

#ID outliers
boxplot.stats(bw$weight)$out
boxplot.stats(bw_na$percent_change_from_July2008)$out

#ID rows containing outliers
out <- boxplot.stats(bw$weight)$out
out_ind <- which(bw$weight %in% c(out))
out_ind

out2<- boxplot.stats(bw_na$percent_change_from_July2008)$out
out_ind2 <- which(bw_na$percent_change_from_July2008 %in% c(out))
out_ind2

bw[out_ind, ]

#create new df without outliers to rerun stats
bw_no_outliers<- bw[-c(155,157,160,161,162,169,175,181,189,193,194, 199, 203, 210, 214, 215, 220, 225), ]

bw_na_no_outliers<- bw_na[-c(215,225), ]


  #id outliers



  bw %>%
    group_by(time_point, sym.spp) %>%
    identify_outliers(weight)

bw_na_no_outliers %>%
  group_by(time_period, sym.spp) %>%
  identify_outliers(weight)

  bw %>%
    group_by(time_point, sym.spp) %>%
    identify_outliers(weight)
  
  
ggqqplot(bw_no_outliers, "weight", ggtheme = theme_bw())+
  facet_grid(time_point~sym.spp, labeller ="label_both")

ggqqplot(bw_na_no_outliers, "percent_change_from_July2008", ggtheme = theme_bw())+
  facet_grid(time_period~sym.spp, labeller ="label_both")

