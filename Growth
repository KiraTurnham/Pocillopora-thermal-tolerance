#theme(legend.position="right") +  
  # labels = c('June 2009', 'September 2009', 'July 2008'))
  
  
  # Testing normality of calc data for D
  with(calc, shapiro.test(G[sym.spp == "D"]))# p = 0.7785
# Shapiro-Wilk normality test for C
with(calc, shapiro.test(G[sym.spp == "C"])) # p = 0.8023
   
#T test
t.test(G ~ sym.spp, data=calc, alternative = "two.sided", var.equal = TRUE)
#p-value = 0.1223
#95 percent confidence interval:
# -0.05331589  0.37998256
#sample estimates:
#mean in group C mean in group D 
#      0.7333333       0.5700000 

